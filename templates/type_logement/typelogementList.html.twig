

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">

    <h1>Liste des types de logement</h1>      

    <div class="contact-content">
        
        <div class="container">
            <a href="/typelogement/add">Ajouter</a>
        </div>

        <div class="container">
            <div class="row">
                <div class="col-lg-5">
                    
                    <table style="width:100%">
                      <tr>
                        <th>Libelle</th>
                      </tr>
                    
                        {% for tl in typelogements %}
                      <tr>
                        <td> {{tl.libelle}} </td>
                        <td> 
                            <a href="/typelogement/add/{{ tl.id }}">Modifier</a>
                            <a href="/typelogement/remove/{{ tl.id }}">Supprimer</a> 
                        </td>
                      </tr>
                      {% endfor %}
                    </table>

                </div>
            </div>
        </div>
    </div>


</div>
{% endblock %}
